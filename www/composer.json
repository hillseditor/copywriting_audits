{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^2.0",
        "drupal/action": "^0.2",
        "drupal/activity_emails": "^1.7",
        "drupal/adaptivetheme": "^5.1",
        "drupal/address": "^1.12",
        "drupal/admin_content_notification": "^4.1",
        "drupal/admin_toolbar": "^3.4",
        "drupal/advban": "^1.6",
        "drupal/at_tool": "^2.0",
        "drupal/autoban": "^1.9",
        "drupal/autologout": "^2.0",
        "drupal/book": "^1.0",
        "drupal/checklistapi": "^2.1",
        "drupal/color": "^1.0",
        "drupal/color-color": "^1.0",
        "drupal/composer_deploy": "^1.7",
        "drupal/config_update": "^2.0@alpha",
        "drupal/contact_storage": "^1.3",
        "drupal/copyprevention": "^2.0@beta",
        "drupal/core-composer-scaffold": "^10.1",
        "drupal/core-project-message": "^10.1",
        "drupal/core-recommended": "^10.1",
        "drupal/csp": "^2.0",
        "drupal/csv_importer": "^1.16",
        "drupal/ctools": "^4.0",
        "drupal/d8w3css": "^2.0",
        "drupal/devel": "^5.1",
        "drupal/disable_user_1": "^1.0",
        "drupal/ds": "^3.22",
        "drupal/easy_breadcrumb": "^2.0",
        "drupal/editor_note": "^1.0",
        "drupal/elogger": "^1.1",
        "drupal/encrypt": "^3.1",
        "drupal/entity": "^1.4",
        "drupal/entity_print": "^2.13",
        "drupal/entity_reference_revisions": "^1.10",
        "drupal/entity_update": "^3.0",
        "drupal/entity_usage": "^2.0@beta",
        "drupal/environment_indicator": "^4.0",
        "drupal/extlink": "^2.0@beta",
        "drupal/field_group": "^3.4",
        "drupal/flood_control": "^2.3",
        "drupal/form_mode_control": "^2.3",
        "drupal/forum": "^1.0",
        "drupal/fullcalendar_view": "^5.1",
        "drupal/gin": "^3.0@RC",
        "drupal/gin_toolbar": "^1.0@RC",
        "drupal/google_tag": "^2.0",
        "drupal/honeypot": "^2.1",
        "drupal/http_response_headers": "^3.0",
        "drupal/imagefield_tokens": "^3.1",
        "drupal/imce": "^3.0",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/jquery_ui": "^1.6",
        "drupal/jquery_ui_accordion": "^2.0",
        "drupal/js_cookie": "^1.0",
        "drupal/key": "^1.17",
        "drupal/lazy": "^4.0",
        "drupal/mailsystem": "^4.4",
        "drupal/message": "^1.4",
        "drupal/message_notify": "^1.3",
        "drupal/metatag": "^2.0",
        "drupal/module_filter": "^5.0",
        "drupal/node_view_permissions": "^1.6",
        "drupal/noreqnewpass": "^1.3",
        "drupal/oembed_providers": "^2.1",
        "drupal/office_hours": "^1.12",
        "drupal/paragraphs": "^1.16",
        "drupal/pathauto": "^1.12",
        "drupal/permissionspolicy": "^1.2",
        "drupal/phpmailer_smtp": "^2.2",
        "drupal/profile": "^1.7",
        "drupal/real_aes": "^2.5",
        "drupal/redirect": "^1.9",
        "drupal/remove_http_headers": "^2.1",
        "drupal/reporting": "^2.0",
        "drupal/responsive_tables_filter": "^2.0",
        "drupal/restui": "^1.21",
        "drupal/robotstxt": "^1.5",
        "drupal/scheduler": "^2.0",
        "drupal/schema_metatag": "^3.0",
        "drupal/search404": "^2.2@beta",
        "drupal/seckit": "^2.0",
        "drupal/seo_checklist": "^5.2",
        "drupal/simple_gmap": "^3.1",
        "drupal/simple_pass_reset": "^1.2",
        "drupal/slick": "^3.0",
        "drupal/smart_date": "^4.0",
        "drupal/smart_ip": "^5.0@alpha",
        "drupal/social_api": "^4.0",
        "drupal/social_auth": "^4.1",
        "drupal/social_auth_google": "^4.0",
        "drupal/state_machine": "^1.10",
        "drupal/statistics": "^1.0",
        "drupal/structure_sync": "^2.0",
        "drupal/tablefield": "^2.5@RC",
        "drupal/taxonomy_manager": "^2.0",
        "drupal/tfa": "^2.0@alpha",
        "drupal/token": "^1.13",
        "drupal/toolbar_menu": "^3.0",
        "drupal/tour": "^1.0",
        "drupal/upgrade_status": "^4.3",
        "drupal/username_enumeration_prevention": "^1.3",
        "drupal/variationcache": "^1.4",
        "drupal/video_embed_field": "^2.5",
        "drupal/views_bulk_operations": "^4.2",
        "drupal/views_slideshow": "^5.0",
        "drupal/webform": "^6.2",
        "drupal/xmlsitemap": "^1.5",
        "drupal/youtube_plus": "^1.0",
        "drupal/youtubechannel": "^3.4",
        "drupal/zeropoint": "^1.14",
        "drush/drush": "^13.0.0-rc1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/installing-drupal",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
